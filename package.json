{
  "name": "gulp-scss-template",
  "version": "1.0.0",
  "description": "projects build on sass syntax with gulp",
  "main": "gulpfile.js",
  "dependencies": {},
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "^1.0.0",
    "autoprefixer": "^9.2.1",
    "browser-sync": "^2.26.5",
    "del": "^3.0.0",
    "eslint": "^5.6.0",
    "eslint-config-htmlacademy": "^0.5.2",
    "gulp": "^3.9.1",
    "gulp-csso": "^3.0.1",
    "gulp-htmlmin": "^5.0.1",
    "gulp-imagemin": "^3.2.0",
    "gulp-plumber": "^1.2.0",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "gulp-svgstore": "^7.0.1",
    "gulp-uglify": "^3.0.2",
    "gulp-uglify-es": "^1.0.4",
    "gulp-webp": "^2.3.0",
    "pump": "^3.0.0",
